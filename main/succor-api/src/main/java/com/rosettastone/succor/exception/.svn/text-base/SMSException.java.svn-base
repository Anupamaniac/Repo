package com.rosettastone.succor.exception;

import com.rosettastone.succor.exception.ApplicationException;

/**
 * The {@code SMSException} can be thrown while sending sms
 * User: Nikolay Sazonov Date: 5/4/11
 */
public class SMSException extends ApplicationException {

    public SMSException(Throwable throwable) {
        super(throwable);
    }

    public SMSException(String detailMessage) {
        super(detailMessage);
    }
}
