package com.rosettastone.succor.model.parature;

/**
 *
 */
public class Action {

    private long ticketId;

    private String type;

    private String comment;

    private boolean showToCust = false;

    private boolean showToAdditionalContact = true;

    public long getTicketId() {
        return ticketId;
    }

    public void setTicketId(long ticketId) {
        this.ticketId = ticketId;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getComment() {
        return comment;
    }

    public void setComment(String comment) {
        this.comment = comment;
    }

    public boolean isShowToCust() {
        return showToCust;
    }

    public void setShowToCust(boolean showToCust) {
        this.showToCust = showToCust;
    }

    public boolean isShowToAdditionalContact() {
        return showToAdditionalContact;
    }

    public void setShowToAdditionalContact(boolean showToAdditionalContact) {
        this.showToAdditionalContact = showToAdditionalContact;
    }
}
