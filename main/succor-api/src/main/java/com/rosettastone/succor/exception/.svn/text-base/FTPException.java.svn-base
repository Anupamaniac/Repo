package com.rosettastone.succor.exception;

import com.rosettastone.succor.exception.ApplicationException;

/**
 * The {@code FTPException} can be thrown while interaction with ftp.
 * User: mixim
 * Date: 7/21/11
 */

public class FTPException extends ApplicationException {
    public FTPException(Throwable throwable) {
        super(throwable);
    }

    public FTPException(String detailMessage) {
        super(detailMessage);
    }

    public FTPException(String detailMessage, Throwable throwable) {
        super(detailMessage, throwable);
    }
}
