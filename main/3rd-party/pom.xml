<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.rosettastone.succor</groupId>
		<artifactId>main</artifactId>
		<version>2.0</version>
	</parent>

	<artifactId>3rd-party</artifactId>
	<name>${artifactId}</name>
	<packaging>pom</packaging>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-install-plugin</artifactId>
				<executions>
					<execution>
						<id>amq-spring-integration-support</id>
						<phase>install</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>org.opencredo.amq</groupId>
							<artifactId>amq-spring-integration-support</artifactId>
							<version>${amq-spring-integration}</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
							<file>lib/amq-spring-integration-support-${amq-spring-integration}.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>amq-template</id>
						<phase>install</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>org.opencredo.amq</groupId>
							<artifactId>amq-template</artifactId>
							<version>${amq-spring-integration}</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
							<file>lib/amq-template-${amq-spring-integration}.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>ojdbc</id>
						<phase>install</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>ojdbc</groupId>
							<artifactId>ojdbc</artifactId>
							<version>${ojdbc}</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
							<file>lib/ojdbc-${ojdbc}.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>drools-guvnor-classes</id>
						<phase>install</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>com.rosettastone.succor</groupId>
							<artifactId>drools-guvnor-classes</artifactId>
							<version>5.1.0</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
							<file>lib/drools-guvnor-classes-5.1.0.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>gdata-core-1.0</id>
						<phase>install</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>com.google.gdata</groupId>
							<artifactId>core</artifactId>
							<version>1.0</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
							<file>lib/gdata-core-1.0.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>gdata-client-1.0</id>
						<phase>install</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>com.google.gdata</groupId>
							<artifactId>client</artifactId>
							<version>1.0</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
							<file>lib/gdata-client-1.0.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>SyncProxy</id>
						<phase>install</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>com.gdevelop.gwt</groupId>
							<artifactId>syncrpc</artifactId>
							<version>0.1.4</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
							<file>lib/SyncProxy-0.1.4.jar</file>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
