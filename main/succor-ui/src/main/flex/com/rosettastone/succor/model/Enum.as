package com.rosettastone.succor.model {

/**
 * spring-flex does not map Enum objects correctly.
 * We map all java Enum values to this model object manually.
 */
[Bindable]
[RemoteClass(alias="com.rosettastone.succor.web.dto.EnumDTO")]
public class Enum {

    public var name:String;
    public var label:String;
}
}
